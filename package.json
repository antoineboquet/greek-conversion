{
  "name": "greek-conversion",
  "author": "Antoine Boquet",
  "license": "agpl-3.0",
  "version": "0.9.2",
  "description": "A small library to convert a polytonic greek string from/into various representations.",
  "keywords": [
    "greek",
    "betacode",
    "transliteration",
    "conversion"
  ],
  "homepage": "https://github.com/antoineboquet/greek-conversion#readme",
  "repository": "github:antoineboquet/greek-conversion.git",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/greekConversion.js",
  "types": "./dist/greekConversion.d.ts",
  "browser": "dist/greekConversion.min.js",
  "targets": {
    "browser": {
      "optimize": true
    }
  },
  "files": [
    "/dist"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  },
  "scripts": {
    "build": "parcel build",
    "prepare": "npm run build",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "parcel": "^2.6.2",
    "ts-jest": "^28.0.7"
  }
}
